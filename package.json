{
  "name": "@ctzhian/tiptap",
  "version": "1.10.1",
  "description": "基于 Tiptap 二次开发的编辑器组件",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "rm -rf dist && father build",
    "build:watch": "father dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "prepare": "husky install && dumi setup",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "pre": "pnpm build && pnpm link --global"
  },
  "authors": [
    "ky.kyy@qq.com"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KuaiYu95/tiptap-v3.git"
  },
  "bugs": {
    "url": "https://github.com/KuaiYu95/tiptap-v3/issues"
  },
  "keywords": [
    "react",
    "tiptap",
    "editor"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/KuaiYu95/tiptap-v3#readme",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/icons-material": "^7",
    "@mui/material": "^7",
    "@mui/utils": "^7",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/diff-match-patch": "^1.0.36",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "dumi": "^2.4.13",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.2",
    "@tiptap/core": "^3.10.1",
    "@tiptap/extension-bubble-menu": "^3.10.1",
    "@tiptap/extension-code": "^3.10.1",
    "@tiptap/extension-code-block-lowlight": "^3.10.1",
    "@tiptap/extension-details": "^3.10.1",
    "@tiptap/extension-drag-handle-react": "^3.10.1",
    "@tiptap/extension-emoji": "^3.10.1",
    "@tiptap/extension-file-handler": "^3.10.1",
    "@tiptap/extension-highlight": "^3.10.1",
    "@tiptap/extension-horizontal-rule": "^3.10.1",
    "@tiptap/extension-image": "^3.10.1",
    "@tiptap/extension-invisible-characters": "^3.10.1",
    "@tiptap/extension-link": "^3.10.1",
    "@tiptap/extension-list": "^3.10.1",
    "@tiptap/extension-mathematics": "^3.10.1",
    "@tiptap/extension-mention": "^3.10.1",
    "@tiptap/extension-subscript": "^3.10.1",
    "@tiptap/extension-superscript": "^3.10.1",
    "@tiptap/extension-table": "^3.10.1",
    "@tiptap/extension-table-of-contents": "^3.10.1",
    "@tiptap/extension-text-align": "^3.10.1",
    "@tiptap/extension-text-style": "^3.10.1",
    "@tiptap/extension-unique-id": "^3.10.1",
    "@tiptap/extension-youtube": "^3.10.1",
    "@tiptap/extensions": "^3.10.1",
    "@tiptap/html": "^3.10.1",
    "@tiptap/markdown": "^3.10.1",
    "@tiptap/pm": "^3.10.1",
    "@tiptap/react": "^3.10.1",
    "@tiptap/starter-kit": "^3.10.1",
    "@tiptap/static-renderer": "^3.10.1",
    "@tiptap/suggestion": "^3.10.1",
    "core-js": "^3.46.0",
    "diff-match-patch": "^1.0.5",
    "highlight.js": "^11.11.1",
    "jszip": "^3.10.1",
    "katex": "^0.16.22",
    "linkifyjs": "^4.3.2",
    "lowlight": "^3.3.0",
    "react-colorful": "^5.6.1",
    "react-image-crop": "^11.0.10",
    "react-photo-view": "^1.2.7",
    "uuid": "^11.1.0"
  }
}
